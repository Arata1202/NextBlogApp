    "@vercel/analytics": "^1.3.1",
    "@vercel/speed-insights": "^1.0.10",
    "cheerio": "^1.0.0-rc.12",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.1",
    "dotenv": "^16.4.5",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "^8.7.0",
    "microcms-js-sdk": "^2.3.2",
    "next": "^15.0.1",
    "next-pwa": "^5.6.0",
